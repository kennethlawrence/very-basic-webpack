{
  "name": "BasicWebpack",
  "version": "0.0.1",
  "description": "A very simple webpack build environment.",
  "main": "dist/main.js",
  "scripts": {
    "test": "node_modules/karma/bin/karma start --single-run",
    "test-continuous": "node_modules/karma/bin/karma start",
    "start": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.development.config",
    "development": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --colors --config webpack.development.config",
    "build": "rm -rf ./build && node_modules/webpack/bin/webpack.js --progress --colors --config webpack.development.config.js",
    "deploy": "rm -rf ./dist && node_modules/webpack/bin/webpack.js --progress --p --config webpack.production.config.js",
    "analyze": "rm -rf ./dist && node_modules/webpack/bin/webpack.js --json -p --config webpack.production.config.js | node_modules/webpack-bundle-size-analyzer/analyze-bundle-size",
    "clean": "rm -rf ./dist && rm -rf ./build"
  },
  "author": "Kenneth Lawrence",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.7.1",
    "image-webpack-loader": "^1.6.3",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs": "^1.9.19",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12",
    "webpack-bundle-size-analyzer": "^1.3.0",
    "webpack-dev-server": "^1.14.1"
  }
}
